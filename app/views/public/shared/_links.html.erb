<%- if controller_name != 'sessions' %>
  <div class="mt-5 mb-5">
    <%= link_to "ユーザーログイン", new_session_path(resource_name) %><br />
  </div>
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <div class="mt-5 mb-5">
    <%= link_to "User新規登録", new_registration_path(resource_name) %><br />
  </div>
<% end %>